   "outputs": [],
   "source": [
    "import requests\n",
    "r = requests.get('http://www.hindiverbs.com/gender.html')"
    "r.encoding = 'utf-8'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "from bs4 import BeautifulSoup\n",
    "soup = BeautifulSoup(r.text, 'html.parser')\n",
    "hindi = soup.find_all('span', attrs={'class':'col3'})\n",
    "gender = soup.find_all('span', attrs={'class':'col4'})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "records=[]\n",
    "for h in hindi[1:]:\n",
    "    for g in gender[1:]:\n",
    "        hindi_word = h.text\n",
    "        gender_word = g.text\n",
    "        records.append((hindi_word, gender_word))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "df = pd.DataFrame(records, columns=['Hindi Script', 'Grammatical Gender'])\n",
    "df.to_csv('hindi_dataset_1.csv', index=False, encoding='utf-8')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
